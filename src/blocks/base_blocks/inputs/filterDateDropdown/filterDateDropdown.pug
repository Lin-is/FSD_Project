mixin filterDateDropdown (name, id)       
    div(class= 'filterDateDropdown__container')
        div(class='filterDateDropdown__labelsContainer' )
            h3(class='filterDateDropdown__nameText' )= name
        label(for='filterDateDropdown-' + `${id}` class='filterDateDropdown__label')
            input(type='text' placeholder='ДД.ММ.ГГГГ' class="filterDateDropdown__input" id='filterDateDropdown-' + `${id}`)
